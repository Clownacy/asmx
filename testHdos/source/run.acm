***	RUN - PROCESS RUN COMMAND.
*
*	RU[N] FNAME [PARAMETER LIST]

RUN	POP	H			; (HL) = COMMAND LINE ADDRESS
	CALL	M$SOB			; MAP + SKIP LEADING BLANKS
	XCHG				; (DE) = PROGRAM NAME
	LXI	H,LINE			; (HL) = LINE BUFFER
	LXI	B,99			; Max count
	CALL	$MOVE			; COPY LINE OVER ITSELF
	LXI	H,LINE
	CALL	FNST			; Find end of word
	PUSH	H			; Save parameter list addr
	CALL	$MOVEL			; Preset default
	DW	6,RUNA,SYSBLK
	JMP	SYSC5.			; PROCESS AS IMPLICIT RUN
