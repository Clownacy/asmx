***	CHECK - CRC FILES.
*
*	CH[ECK] FNAME [,FNAME,...,FNAME]
*	CRC FNAME [,FNAME,...,FNAME]

CHECK	POP	H
	CALL	$SOB
	ANA	A
	JZ	ILLSYN

	MOV	B,H
	MOV	C,L
	PUSH	B			; SAVE COMMAND ADDRESS
	CALL	FEC			; FIND END OF COMMAND LINE
	LXI	B,CHEAL
	LXI	D,CHEA
	CALL	$MOVE			; ADD '/CRC' COMMAND
	POP	B
	JMP	PIP

CHEA	DB	'/CRC',0
CHEAL	EQU	*-CHEA
