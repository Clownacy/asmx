***	RENAME - RENAME FILES.
*
*	REN[AME] NEWNAME=OLDNAME

RENAME	POP	H
	CALL	$SOB
	ANA	A
	JZ	ILLSYN

	MOV	B,H
	MOV	C,L
	PUSH	B			; (BC) = START OF COMMAND
	CALL	FEC			; FIND END OF COMMAND
	LXI	B,RENAL
	LXI	D,RENA
	CALL	$MOVE			; MOVE IN '/RENAME'
	POP	B
	JMP	PIP

RENA	DB	'/R',0
RENAL	EQU	*-RENA
